@using Vidly2.Models
@model System.Collections.Generic.IEnumerable<Vidly2.Models.Movie>

@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table>
    <thead>
    <tr>
        <td>
            Movies
        </td>
    </tr>
    </thead>
    <tbody>
    @foreach (Movie movie in Model)
    {
        <tr><td>@movie.Name</td></tr>
    }
    </tbody>
</table>
